<?xml version="1.0" encoding="utf-8"?>

<!-- This is PerfConfigsStore file which returns the value
when a particular property name is invoked.
This file supports only Prop tag, which has the following
fields.
* Name, to identify the name of the property.
The names of the properties have been classified into
Vendor and System property.
* Value, the new value to be returned as string when
respective property Name is called. -->

<PerfConfigsStore>
    <PerfConfigs>
        
        <!-- /vendor/bin/hw/vendor.qti.hardware.perf@2.2-service -->
        <Prop Name="vendor.debug.enable.lm" Value="false" /> <!-- false because doesn't use /vendor/etc/lm/ -->
        <Prop Name="vendor.debug.enable.memperfd" Value="true" />
        
        <!-- /vendor/lib(64)/libqti-perfd.so -->
        <Prop Name="vendor.perf.fps_switch_hyst_time_secs" Value="10" />
        <Prop Name="ro.vendor.perf.splh" Value="none" />
        
    </PerfConfigs>
</PerfConfigsStore>
